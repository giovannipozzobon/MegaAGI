    "version": "0.2.0",
    "configurations": [
        {
            "name": "(Windows) Launch",
            "type": "lldb",
            "request": "launch",
            "program": "/Applications/Mega65/bin/xmega65",
            "args": ["-8", ".\\agi.d81", "-uartmon", ":4510"],
            "cwd": "${workspaceRoot}",
            "console": "internalConsole"
        },
        {
            "name": "(MEGA65) Launch",
            "type": "lldb",
            "request": "launch",
            "program": "/Applications/m65tools/etherload.osx",
            "args": ["-r", "-m", "agi.d81", "-5", "--ntsc", "agi.prg"],
            "cwd": "${workspaceRoot}",
            "console": "internalConsole"
        },
   ]
}